	"workbench.editor.limit.value": 8,
	"workbench.editor.mouseBackForwardToNavigate": false,
	"workbench.editor.openPositioning": "last",
	"workbench.editor.revealIfOpen": true,
	"workbench.editor.splitSizing": "split",
	"workbench.editor.tabSizing": "shrink",
	"workbench.editor.titleScrollbarSizing": "large",
	"workbench.editorAssociations": {
		"*.md": "vscode.markdown.preview.editor",
		"*.wasm": "default",
		"{git,gitlens}:/**/*.{md,csv}": "default",
		"{git,gitlens}:/**/*.{md,csv,svg}": "default"
	},
	"workbench.experimental.enableNewProfilesUI": true,
	"workbench.list.defaultFindMode": "filter",
	"workbench.view.alwaysShowHeaderActions": true
